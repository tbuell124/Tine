{
  "name": "Tine",
  "slug": "tine",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "automatic",
  "splash": {
    "image": "./assets/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "assetBundlePatterns": ["**/*"],
  "jsEngine": "hermes",
  "ios": {
    "supportsTablet": true,
    "buildNumber": "1.0.0",
    "infoPlist": {
      "NSMicrophoneUsageDescription": "TunePlay uses the microphone to analyze instrument pitch in real time.",
      "UIBackgroundModes": ["audio"]
    }
  },
  "android": {
    "versionCode": 1,
    "permissions": ["RECORD_AUDIO", "VIBRATE"],
    "newArchitecture": { "enabled": true },
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    }
  },
  "web": { "bundler": "metro", "favicon": "./assets/favicon.png" },
  "plugins": [
    [
      "expo-build-properties",
      {
        "android": {
          "newArchEnabled": true,
          "enableProguardInReleaseBuilds": true,
          "extraProguardRules": "-keep class com.facebook.react.turbomodule.** { *; }"
        },
        "ios": { "newArchEnabled": true }
      }
    ]
  ],
  "_internal": {
    "isDebug": false,
    "projectRoot": "/Users/tylerbuell/Projects/Tine",
    "dynamicConfigPath": {},
    "staticConfigPath": "/Users/tylerbuell/Projects/Tine/app.json",
    "packageJsonPath": "/Users/tylerbuell/Projects/Tine/package.json"
  },
  "sdkVersion": "51.0.0",
  "platforms": ["ios", "android", "web"]
}
